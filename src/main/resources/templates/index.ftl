<div class="container-fluid">
    <div class="home-slide row">
        <div class="navigation">
            <h1 class="sr-only">
                EA PHOTOGRAPHER
            </h1>
            <div class="logo"></div>
            <div>
                <div class="navigation-buttons">
                    <a class="btn btn-home" id="gallery">Gallery</a>
                    <a class="btn btn-home" id="backstages">Backstages</a>
                    <a class="btn btn-home" id="contacts">Contacts</a>
                </div>
                <div class="socials">
                    <a href="https://www.instagram.com/_ea_photographer_/" class="socials-item icon-instagram-filled" target="_blank">instagram</a>
                    <a href="https://www.facebook.com/katyeaphotographer/" class="socials-item icon-facebook-rect" target="_blank">facebook</a>
                    <a href="https://www.youtube.com/user/DamageClub" class="socials-item icon-youtube" target="_blank">youtube</a>
                    <a href="https://vk.com/ea_photographer" class="socials-item icon-vkontakte-rect" target="_blank">vkontakte</a>
                    <a href="" class="socials-item icon-mail-alt">mail</a>
                    <a href="" class="socials-item icon-viber">viber</a>
                </div>
            </div>
        </div>
    </div>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js" defer></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js" defer></script>
    <div class="row">
        <div class="col-sm-2 col-xs-12 find">
            <div class="find-container col-xs-12 col-sm-9">
                <div class="icon-search clickable search">
                    <div id="find-input" class="ui-dialog ui-corner-all ui-widget ui-widget-content ui-front ui-dialog-buttons" title="Find by tags"><form><input type="text" class="form-control" placeholder="Enter tags here" id="find-text"></form></div>
                </div>
                <div class="icon-dot-3 clickable see-all">See all</div>
            </div>
            <div class="col-sm-3 hidden-xs find-trigger trigger-off">
            </div>
        </div>
        <div class="gallery col-sm-8 col-xs-12">
            <h2 class="sr-only">Gallery<a href="#gallery"></a></h2>
            <#list categories as category>
                <#if (category?index < 3)>
                    <div class="category row">
                        <h3 alt="${category.id}">${category.name} <span class="category-see-all clickable">&#xe803;</span></h3>
                        <div class="items nonexpanded">
                            <#list category.photos as photo>
                                <div><img src="${photo.photo}" class='gallery-img' alt="${photo.tags}"></div>
                            <#else>
                                <h4>The category is empty yet...</h4>
                            </#list>
                        </div>
                        <hr>
                    </div>
                <#else>
                    <div class="category row">
                        <h3 alt="${category.id}">${category.name} <span class="category-see-all clickable">&#xe803;</span></h3>
                        <div class="items nonexpanded">
                            <#list category.photos as photo>
                                <div><img src="/img/loading.svg" class='gallery-img' alt="${photo.tags}"></div>
                            <#else>
                                <h4>The category is empty yet...</h4>
                            </#list>
                        </div>
                        <hr>
                    </div>
                </#if>
            <#else>
                <div style="height:100%;"><h3 style="text-align:center;">There is no categories yet.</h3></div>
            </#list>
        </div>
    </div>


    <div class="backstages row">
        <div class="col-xs-12 col-sm-8 col-sm-offset-2">
            <div class="backstages-container">
                <#list backstages as backstage>
                    <div class="backstages-item">
                        <h3>${backstage.name}</h3>
                        <img src="${backstage.mainPhoto}" alt="${backstage.name}" class="backstages-img">
                        <div class="backstage-photos backstage-non-expanded">
                            <#list backstage.backstagePhotos as photo>
                                <div><img src="/img/loading.svg" alt="${backstage.name}" class="backstage-photos-img"></div>
                            </#list>
                        </div>
                    </div>
                <#else>
                    <div><h3 style="text-align:center;">There is no backstages yet.</h3></div>
                </#list>
            </div>
        </div>
    </div>
    <script src="/js/scripts.min.js" defer></script>
    <div class="row contacts">
        <div class="col-xs-12 col-sm-10 col-lg-8 col-sm-offset-1 col-lg-offset-2">
            <div class="contact-block">
                <h3 class="icon-location">Location</h3>
                <p>Poznań, Polska</p>
            </div>
            <div class="contact-block">
                <h3 class="icon-mail-alt">Email</h3>
                <p>Damageclub@gmail.com</p>
            </div>
            <div class="contact-block">
                <h3 class="icon-viber">Phone</h3>
                <p>+48 795 019 708</p>
            </div>
        </div>
    </div>
</div>
<link rel="stylesheet" href="https://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
